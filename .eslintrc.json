{
    "extends": [ 
      "airbnb",
      "prettier",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars":"warn",
      "no-console":"warn",
      "func-names":"off",
      "react/prop-types": [0]
    },
    "globals": {
      "React": true
    }
}
